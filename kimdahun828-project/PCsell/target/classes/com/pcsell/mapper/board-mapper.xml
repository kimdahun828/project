<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcsell.mapper.BoardMapper">
	
	<select id="findList" resultType="com.pcsell.vo.board">
		SELECT title, content, ID
		FROM BOARD
	</select>
	
	<select id="loseDetail" parameterType="int" resultType="com.pcsell.vo.board">
		SELECT title, content
		FROM BOARD
	</select>
	
	
	<resultMap id="boardMap" type="com.pcsell.vo.board">
		<id column="no" property="no" />
		<result column="title" property="title" />
		<result column="content" property="content" />
		<result column="ID" property="ID" />
	</resultMap>

</mapper>